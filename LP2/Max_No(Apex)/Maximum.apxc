public class Maximum {
    public integer firstno{get;set;}
    public integer secondno{get;set;}
    public integer thirdno{get;set;}
    public integer maxno{get;set;}
    public void calculate_max(){
    if((firstno >= secondno) && (firstno >= thirdno)){
        maxno = firstno;    
    }
    else if((secondno >= firstno) &&(secondno >= thirdno)){
        maxno = secondno;  
    }
    else{
        maxno = thirdno;
            
     }
    }
    
}
